<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.crm.model.Kisi" table="KISI">
        <id name="COid" column="ID">
            <generator class="increment"/>
        </id>
        <property name="ad" column="AD" type="string"></property>
        <property name="soyad" column="SOYAD" type="string"></property>
        <property name="kulAdi" column="KULLANICI_ADI" type = "string" />
        <property name="sifre" column="SIFRE" type = "string" />
        <property name="telefon" column="TELEFON" type = "string" />
        <property name="kayitTarihi" column="KAYIT_TARIHI" type = "timestamp" />
  </class>
</hibernate-mapping>